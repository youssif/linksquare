<h1>Listing alums</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Cohort</th>
    <th>Linkedin url</th>
    <th>Twitter handle</th>
    <th>Github link</th>
    <th>City</th>
    <th>State</th>
    <th>Title</th>
    <th>Picture</th>
    <th>Expertise</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alums.each do |alum| %>
  <tr>
    <td><%= alum.first_name %></td>
    <td><%= alum.last_name %></td>
    <td><%= alum.cohort %></td>
    <td><%= alum.linkedin_url %></td>
    <td><%= alum.twitter_handle %></td>
    <td><%= alum.github_link %></td>
    <td><%= alum.city %></td>
    <td><%= alum.state %></td>
    <td><%= alum.title %></td>
    <td><%= alum.picture %></td>
    <td><%= alum.expertise %></td>
    <td><%= link_to 'Show', alum %></td>
    <td><%= link_to 'Edit', edit_alum_path(alum) %></td>
    <td><%= link_to 'Destroy', alum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Alum', new_alum_path %>
